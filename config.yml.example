ConfigFolder: './Demos'
Concurrency:
  ConcurrentTasks: 4
  TreadsPerTask: 5
TaskHistoryDepth: 50
Vlan:
  Id: [
    2000,2001,2002
  ]
  Prefix: 'dod-'
API:
  PathPrefix: ''
  Port: 8080
  Token:
    Secret: 'supersecretvalue'
    IssuerClaim: 'dod'
    ExpirationTime: '3600'
  SuperUser:
    User: 'root'
    Password: 'root'
Router:
  User: ''
  Password: ''
  Port: 22
LogPath:
  Access: 'log/access.log'
  Error: 'log/error.log'
  Info: 'log/info.log'
VMware:
  APITimeout: 60
  URL: ''
  User: ''
  Password: ''
  Insecure: 'true'
  DataCenter: ''
  DemoFolder: ''
  Pool: ''
  Hosts: []
  Vswitch: ''
PostgreSQL:
  Host:      ''
  User:     ''
  Password: ''
  Database: 'dod_db'
  Port: 5432
LDAP:
  URL: 'ldaps://example.com:636'
  BindDN: 'CN=dod-user,DC=example,DC=com'
  BindPassword: ''
  InsecureSkipVerify: true
  UsernameAttribute: 'samaccountname'
  UserGroup:
    UsersDN: 'OU=users,CN=dod-user,DC=example,DC=com'
  AdminGroup:
    UsersDN: 'OU=admins,CN=dod-user,DC=example,DC=com'
